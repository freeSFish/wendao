[broadcast_setting]
broadcast_list : ({ "during_task", "start_task", "end_task" })

[start_task]
broadcast_start : "start_time(0)"
interval        : 0
broadcast_end   : "start_time(1)"
message         : "#R【迎新年】万家灯火#n活动开始了，大家快去#P活动大使#P处领取点亮灯笼的任务吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(3600)"
interval        : 3600
broadcast_end   : "end_time(-1)"
message         : "#R【迎新年】万家灯火#n活动正在火热进行中，大家快去#P活动大使#P处领取任务，把各地的灯笼都点亮起来吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "#R【迎新年】万家灯火#n活动结束啦，在大家的共同努力下各地的灯笼都被点亮了！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


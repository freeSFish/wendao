#FORM2

[活动时间]
time_list$ : ({ "2015-04-10-00:00:00", "EVERY_DAY(24)", 0, "2015-05-03-23:59:59" })

[broadcast_setting]
broadcast_list : ({ "start_task", "during_task", "end_task" })

[start_task]
broadcast_start : "start_time"
interval        : 0
broadcast_end   : "start_time"
message         : "#R【九周年】狐生九尾#n活动开始了，#Z官道北#Z的灵狐经过千百年的修炼，竟生九尾而成妖！在九周年活动期间，#R妖狐#n妄想入侵天墉城，中洲的道友们，快去#Z官道北(24,16)#Z阻止它吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(3600)"
interval        : 3600
broadcast_end   : "end_time(-1)"
message         : "#R【九周年】狐生九尾#n活动正在进行中，#Z官道北#Z的灵狐经过千百年的修炼，竟生九尾而成妖！在九周年活动期间，#R妖狐#n妄想入侵天墉城，中洲的道友们，快去#Z官道北(24,16)#Z阻止它吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time"
message         : "#R【九周年】狐生九尾#n活动已经结束了，妄想入侵的#R妖狐#n已经被驱逐！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


#FORM2

[setting]
start_time       : "2012-12-10-00:00:00"
end_time         : "2012-12-12-23:59:59"

[broadcast_setting]
broadcast_list : ({ "start_task", "end_task", "during_task" })

[start_task]
broadcast_start : "start_time(0)"
interval        : 0
broadcast_end   : "end_time"
message         : "上元佳节，赏灯猜谜，喜乐闹元宵！#R元宵节#n活动开始啦，大家快去#P活动大使#P处了解活动详情吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "#R元宵节#n活动结束啦，祝大家每天都开心快乐！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(+1)"
interval        : 3600
broadcast_end   : "end_time(-1)"
message         : "元宵佳节到了，#P活动大使#P准备了大量的元宵美食想要送给各地的居民们，快去帮#P活动大使#P送汤圆吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


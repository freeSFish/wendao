#FORM2

[活动时间]
time_list$ : ({ "2012-08-23-20:00:00", "EVERY_HOUR(2)", 0, "2012-08-23-22:00:00" })

[broadcast_setting]
broadcast_list : ({ "before_start_task", "start_task", "during_task", "before_end_task" })

[before_start_task]
broadcast_start : "start_time(-1800)"
interval        : 600
broadcast_end   : "start_time(-1)"
message         : "迢迢牵牛星，皎皎河汉女。今晚牛郎织女能不能顺利相会，还要靠大家的努力。#R鹊桥相会#n活动将于#R20:00#n开始，请大家到#Z风月谷#Z做好准备。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[start_task]
broadcast_start : "start_time(0)"
interval        : 0
broadcast_end   : "start_time(1)"
message         : "#R鹊桥相会#n活动开始了，请大家到#Z风月谷#Z教训#R捣乱的喜鹊#n协助搭建#R鹊桥#n。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(1800)"
interval        : 1800
broadcast_end   : "end_time(-1)"
message         : "#R鹊桥相会#n活动正在进行中，请大家到#Z风月谷#Z教训#R捣乱的喜鹊#n协助搭建#R鹊桥#n，助牛O织女顺利相会。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[before_end_task]
broadcast_start : "end_time(-900)"
interval        : 300
broadcast_end   : "end_time(-1)"
message         : "#R鹊桥相会#n活动即将于#R22:00#n结束，请大家抓紧时间。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


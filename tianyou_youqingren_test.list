#FORM2

[setting]
start_time       : "2013-12-09-07:00:00"
end_time         : "2013-12-11-23:59:59"

[broadcast_setting]
broadcast_list : ({ "end_task", "during_task" })

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "单丝不成线，独木难成林。在众道友的合力之下，嫉妒成性的妖物终于被赶跑了！众爱侣皆全身而退，无有损伤，实是万幸！值此花好月圆之际，让我们及时行乐，共享这温馨浪漫的美好时光。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(0)"
interval        : 3600
broadcast_end   : "end_time(-600)"
message         : "#Z风月谷#Z景色旖旎，恰逢佳期，有情之人皆相约于此，你侬我侬，缠绵缱绻，诉尽相思。可恨妖物竟见不得这般美好，从各路闻风而来，密谋偷袭众情侣！如此良辰，怎容邪孽作祟！还请众道友仗义出手，使有情人成眷属，不负痴缘！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


[活动时间]
time_list$ : ({ "2016-07-23-00:00:00", "EVERY_DAY(9)", 0, "2016-07-31-23:59:59" })

[broadcast_setting]
broadcast_list : ({ "end_task", "during_task" })

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "#R【国庆节】勇往直前#n活动已经结束了，#R鸿钧老祖座下的宠物们#n已经离开天墉城，感谢大家的热情参与！#0"

total_end_time : "2016-02-14-23:59:59"

#必须从19：00开始配置
[活动时间]
time_list$ : ({ "2016-02-12-19:00:00", "EVERY_HOUR(2)", "EVERY_DAY(1)", "2016-02-14-23:59:59" })

[broadcast_setting]
broadcast_list : ({ "befor_start", "during_task" })

[befor_start]
broadcast_start : "start_time(-1800)"
interval        : 600
broadcast_end   : "start_time"
message         : "广大情侣们注意啦！#R【情人节】约会大作战#n活动将在#RSTART_TIME#n后开启，如此佳期，幽会于#Z风月谷#Z，岂不美哉！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time"
interval        : 600
broadcast_end   : "end_time(-1)"
message         : "#Z风月谷#Z风景优美，气氛浪漫，堪称最佳约会地点。大家快到#Z风月谷#Z参加#R【情人节】约会大作战#n活动吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


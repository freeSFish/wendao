#FORM2

[setting]
start_time       : "2016-04-15-00:00:00"
end_time         : "2016-05-08-23:59:59"

[broadcast_setting]
broadcast_list : ({ "during_task", "end_task" })

[during_task]
broadcast_start : "start_time(0)"
interval        : 3600
broadcast_end   : "end_time"
message         : "#R【十年问道】猴哥快跑#n活动正在火热进行中！轻松快跑，奖励多多，赶紧打开#@猴哥快跑|Open:GatherHerbsDlg#@一起跑起来吧！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time"
interval        : 0
broadcast_end   : "end_time(+1)"
message         : "本次#R【十年问道】猴哥快跑#n圆满结束了，感谢各位道友捧场！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

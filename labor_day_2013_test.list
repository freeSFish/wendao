#FORM2

[活动时间]
time_list$ : ({ "2013-03-11-00:00:00", "EVERY_HOUR(21)", "EVERY_DAY(1)", "2013-03-13-20:59:59" })

[broadcast_setting]
broadcast_list : ({ "during_task1", "during_task2", "end_task" })

[during_task1]
broadcast_start : "start_time(0)"
interval        : 1200
broadcast_end   : "end_time(-8000)"
message         : "劳动节活动#R【劳动节】劳动最光荣#n第一阶段活动开始了，大家快去#Z天墉城#Z找到地面上出现的#Y裂缝#n和#Y杂草#n，将它们消灭后即可获得丰厚的数值奖励。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task2]
broadcast_start : "end_time(-7200)"
interval        : 1200
broadcast_end   : "end_time(-1)"
message         : "劳动节活动#R【劳动节】劳动最光荣#n第二阶段活动开始了，大家快去#Z天墉城(158,266)#Z击杀企图破坏劳动成果的#Y慵懒魔王#n或#Y慵懒妖王#n，战斗胜利后可获得丰厚的数值奖励，还有机会获得珍贵的商城道具。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "今天的#R劳动节活动#n已全部结束，感谢大家的参与！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


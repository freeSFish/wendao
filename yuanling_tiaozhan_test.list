#FORM2

[活动时间]
time_list$ : ({ "2015-12-07-00:00:00", "EVERY_DAY(7)", "EVERY_WEEK(0)", "2015-12-13-23:59:59" })

[broadcast_setting]
broadcast_list : ({ "end_task", "during_task" })

[end_task]
broadcast_start : "end_time(-1)"
interval        : 0
broadcast_end   : "end_time(0)"
message         : "#R【春节】元灵挑战#n活动如期结束了，众道友都通过与#R三只元灵#n的交手，提高了自身的道行修为，真是太好了！#0"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(0)"
interval        : 3600
broadcast_end   : "end_time(-3599)"
message         : "值此新年之际，#Y颛顼#n给全中洲玩家送福啦！#Y颛顼#n派出了#R三只元灵#n在#Z天墉城(248,202)#Z附近迎接各道友的挑战，大家快去#P活动大使#P处领取#R【春节】元灵挑战#n任务吧。#50"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[normal]
polar            : -1
religion         : -1
life_effect      : "(500 + (LV / 100) * 40 + (LV / 130) * 60 + STAGE * 45) * 8 / 10"

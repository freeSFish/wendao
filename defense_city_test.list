[broadcast_setting]
broadcast_list : ({ "before_start_task", "start_task" })

[before_start_task]
broadcast_start : "start_time(-3600)"
interval        : 300
broadcast_end   : "start_time(-1)"
message         : "#R今天START_TIME#n将有大批的妖魔集结在一起，从#Z官道北#Z和#Z官道南#Z分别向#Z天墉城#Z和#Z揽仙镇#Z发起进攻，请侠义之士迅速赶到#R三线#n、#R四线#n的#Z官道北#Z、#Z官道南#Z击杀年兽，保卫城镇。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[start_task]
broadcast_start : "start_time"
interval        : 0
broadcast_end   : "end_time"
message         : "妖魔大军开始向#Z天墉城#Z和#Z揽仙镇#Z进发，请侠义之士迅速赶到#R三线#n、#R四线#n的#Z官道北#Z、#Z官道南#Z击杀年兽，保卫城镇。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"


#FORM2

[活动时间]
time_list$ : ({ "2016-06-09-00:00:00", "EVERY_DAY(3)", "EVERY_WEEK(0)", "2016-06-12-00:00:00" })

[broadcast_setting]
broadcast_list : ({ "during_task", "end_task" })

[during_task]
broadcast_start : "start_time(0)"
interval        : 3600
broadcast_end   : "end_time(-1)"
message         : "#R【中秋】守护嫦娥#n活动开始啦，大家快前往#P嫦娥仙子|天墉城(261,199)#P处挑战四周的#R护卫#n，挑战成功可获得丰厚的奖励哦！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time(0)"
interval        : 0
broadcast_end   : "end_time(1)"
message         : "#R【中秋】守护嫦娥#n活动圆满结束啦，感谢大家的参与。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

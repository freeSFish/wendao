#FORM2

[活动时间]
time_list$ : ({ "2015-06-22-00:00:00", "EVERY_DAY(40)", 0, "2015-07-31-23:59:59" })

[broadcast_setting]
broadcast_list:   ({ "start_task", "end_task", "during_task" })

[start_task]
broadcast_start : "start_time"
interval        : 0
broadcast_end   : "end_time(-60)"
message         : "#R【暑假】火眼金睛#n活动开始啦，#Z天墉城#Z中出现了许多妖魔乔装的NPC，大家快去将它们消灭。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[end_task]
broadcast_start : "end_time"
interval        : 0
broadcast_end   : "end_time(+60)"
message         : "本次#R【暑假】火眼金睛#n活动圆满结束，请关注暑假期间的其他活动！"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

[during_task]
broadcast_start : "start_time(+1200)"
interval        : 1200
broadcast_end   : "end_time(-1000)"
message         : "#R【暑假】火眼金睛#n活动开始啦，#Z天墉城#Z中出现了许多妖魔乔装的NPC，大家快去将它们消灭，同时还可以获得丰厚的道具奖励哦。"
channel         : "CHANNEL_SYSTEM | CHANNEL_NO_BROADCAST"

